{
  "presets": [
    ["env", {
      "targets": {
        "node": "7.6"
      },
      "modules": false,
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true
    }]
  ]
}
